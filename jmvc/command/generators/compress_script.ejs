MVCOptions = {
    onload: false,
    compress_callback: function(total){
        var compressed = MVCOptions.collect_and_compress(total);
        MVCOptions.save('apps/<%= application_name %>/production.js', compressed);
        print("Compressed to 'apps/<%= application_name %>/production.js'.");
        new MVCObject.DApplication(total, "<%= application_name %>");
    }
}
load('jmvc/rhino/htmlparser.js');
load('jmvc/rhino/env.js');
load('jmvc/rhino/helpers.js');
load('jmvc/rhino/doc.js');
window.location = 'apps/<%= application_name %>/index.html';
